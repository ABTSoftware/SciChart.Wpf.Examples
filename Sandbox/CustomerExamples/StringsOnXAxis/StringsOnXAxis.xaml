<Window x:Class="StringsOnXAxisExample.StringsOnXAxis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:s="http://schemas.abtsoftware.co.uk/scichart"
        mc:Ignorable="d"
        Title="StringsOnXAxis" Height="450" Width="800">
    <Grid>
        <s:SciChartSurface x:Name="scs">
            <s:SciChartSurface.RenderableSeries>
                <s:FastColumnRenderableSeries Fill="#FF6600" x:Name="columnSeries"/>
            </s:SciChartSurface.RenderableSeries>
            <s:SciChartSurface.XAxis>
                <s:NumericAxis AutoTicks="False" MajorDelta="1" MinorDelta="0.2"/>
            </s:SciChartSurface.XAxis>
            <s:SciChartSurface.YAxis>
                <s:NumericAxis/>
            </s:SciChartSurface.YAxis>
        </s:SciChartSurface>
    </Grid>
</Window>
