<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Main Window -->
    
    <SolidColorBrush x:Key="MetroWindowTitleBrush" Color="#FFE5E5E5"/>
    <SolidColorBrush x:Key="MetroWindowHeaderBrush" Color="#FF232426"/>
    <SolidColorBrush x:Key="MetroNonActiveWindowHeaderBrush" Color="#FF2C3135"/>

    <SolidColorBrush x:Key="MetroWindowButtonForegroundBrush" Color="#FFE5E5E5"/>
    <SolidColorBrush x:Key="MetroWindowButtonBackgroundBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="MetroWindowButtonMouseOverBrush" Color="#FF151515"/>

    <SolidColorBrush x:Key="MetroWindowDialogViewBackgroundBrush" Color="#90000000"/>

    <Color x:Key="ApplicationBackgroundColor">#FF1C1C1C</Color>
    <SolidColorBrush x:Key="ApplicationBackgroundBrush" Color="#FF1C1C1C"/>

    <SolidColorBrush x:Key="HeadingTextBrush" Color="#FFDFE0E6"/>

    <!-- Hyperlinks -->

    <SolidColorBrush x:Key="HyperlinkForegroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="HyperlinkAlternativeForegroundBrush" Color="#FF42B649"/>

    <!-- Scrollbars -->

    <SolidColorBrush x:Key="ScrollBarThumbBackgroundBrush" Color="#FF8C8C8C"/>
    <SolidColorBrush x:Key="ScrollBarTrackBackgroundBrush" Color="#706C6C6C"/>

    <!-- Search Box -->

    <SolidColorBrush x:Key="SearchBoxBorderBrush" Color="#FF323539"/>
    <SolidColorBrush x:Key="SearchBoxBackgroundBrush" Color="#FF232426"/>
    <SolidColorBrush x:Key="SearchBoxWatermarkForegroundBrush" Color="#FFDFE0E6"/>
    <SolidColorBrush x:Key="SearchBoxItemMouseOverBackgroundBrush" Color="#FF42B649"/>

    <!-- Navigation Buttons (Prev, Home, Next + Settings) -->

    <SolidColorBrush x:Key="NavigationButtonForegroundBrush" Color="#50FFFFFF"/>
    <SolidColorBrush x:Key="NavigationButtonBackgroundBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="NavigationButtonBorderBrush" Color="#FF323539"/>

    <SolidColorBrush x:Key="NavigationButtonMouseOverForegroundBrush" Color="#80FFFFFF"/>
    <SolidColorBrush x:Key="NavigationButtonMouseOverBackgroundBrush" Color="#FF232426"/>
    <SolidColorBrush x:Key="NavigationButtonMouseOverBorderBrush" Color="#FF525559"/>

    <!-- Navigation TabControl (2D Charts, 3D Charts, Featured Apps) -->

    <SolidColorBrush x:Key="NavigationTabControlBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="NavigationTabControlBorderBrush" Color="#FF343A44"/>
    <SolidColorBrush x:Key="NavigationTabControlSliderBrush" Color="#FF42B649"/>

    <SolidColorBrush x:Key="NavigationTabControlItemForegroundBrush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="NavigationTabControlItemSelectedForegroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="NavigationTabControlItemMouseOverBackgroundBrush" Color="#FF232426"/>

    <!-- Navigation TreeView (2D Charts, 3D Charts, Featured Apps) -->

    <SolidColorBrush x:Key="NavigationTreeViewBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="NavigationTreeViewItemSelectedBackgroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="NavigationTreeViewItemHighlightedBackgroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="NavigationTreeViewItemMouseOverBackgroundBrush" Color="#FF232426"/>

    <!-- Chart Icons -->

    <SolidColorBrush x:Key="ChartIconStrokeBrush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="ChartIconColor1Brush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="ChartIconColor2Brush" Color="#FFE3E3E3"/>

    <!-- Showcase -->

    <SolidColorBrush x:Key="ShowcaseExampleHeaderTextBrush" Color="#FFDFE0E6"/>
    <SolidColorBrush x:Key="ShowcaseExampleDescriptionTextBrush" Color="#FFC0C2C3"/>

    <SolidColorBrush x:Key="ShowcaseControlItemBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="ShowcaseControlItemBorderBrush" Color="#FF343A44"/>
    <SolidColorBrush x:Key="ShowcaseControlItemMouseOverBorderBrush" Color="#FF4A8156"/>
    <SolidColorBrush x:Key="ShowcaseControlItemSelectedBorderBrush" Color="#FF42B649"/>

    <SolidColorBrush x:Key="ShowcaseControlScrollButtonBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="ShowcaseControlScrollButtonMouseOverBackgroundBrush" Color="#FF232426"/>
    <SolidColorBrush x:Key="ShowcaseControlScrollButtonBorderBrush" Color="#FFE3E3E3"/>

    <!-- Example List -->

    <SolidColorBrush x:Key="ExampleListGroupSeparatorBrush" Color="#FF606060"/>
    <SolidColorBrush x:Key="ExampleListBusyIndeterminateBrush" Color="#FF42B649"/>
    
    <SolidColorBrush x:Key="ExampleListSortComboBoxBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="ExampleListSortComboBoxSortLabelBrush" Color="#50FFFFFF"/>

    <SolidColorBrush x:Key="ExampleListItemBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="ExampleListItemBorderBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="ExampleListItemForegroundBrush" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="ExampleListItemMouseOverBackgroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="ExampleListItemMouseOverBorderBrush" Color="Transparent"/>

    <!-- Breadcrumb -->

    <SolidColorBrush x:Key="BreadcrumbForegroundBrush" Color="#50FFFFFF"/>
    <SolidColorBrush x:Key="BreadcrumbMouseOverForegroundBrush" Color="#FFC0C2C3"/>
    <SolidColorBrush x:Key="BreadcrumbSelectedForegroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="BreadcrumbSeparatorBrush" Color="#FF323539"/>

    <!-- Main Toolbar -->

    <SolidColorBrush x:Key="MainToolbarForegroundBrush" Color="#50FFFFFF"/>
    <SolidColorBrush x:Key="MainToolbarMouseOverForegroundBrush" Color="#FFC0C2C3"/>
    <SolidColorBrush x:Key="MainToolbarSelectedForegroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="MainToolbarSeparatorBrush" Color="#FF323539"/>

    <!-- Example Navigation View -->

    <SolidColorBrush x:Key="ExampleNavigationViewBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="ExampleNavigationViewSeparatorBrush" Color="#FF606060"/>
    <SolidColorBrush x:Key="ExampleNavigationViewForegroundBrush" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="ExampleNavigationViewItemBackgroundBrush" Color="#FF232426"/>
    <SolidColorBrush x:Key="ExampleNavigationViewMouseOverItemBackgroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="ExampleNavigationViewSelectedItemBackgroundBrush" Color="#FF42B649"/>

    <!-- Settings View -->

    <SolidColorBrush x:Key="SettingsViewBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="SettingsViewSeparatorBrush" Color="#FF606060"/>
    <SolidColorBrush x:Key="SettingsViewForegroundBrush" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="DevModeButtonForegroundBrush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="DevModeButtonBackgroundBrush" Color="#FF232426"/>
    <SolidColorBrush x:Key="DevModeButtonBorderBrush" Color="#FF5B5B5B"/>

    <SolidColorBrush x:Key="DevModeButtonMouseOverForegroundBrush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="DevModeButtonMouseOverBackgroundBrush" Color="#FF333436"/>
    <SolidColorBrush x:Key="DevModeButtonMouseOverBorderBrush" Color="#FF7B7B7B"/>

    <!-- Feedback / Export View -->

    <SolidColorBrush x:Key="FeedbackExportViewBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="FeedbackExportViewSeparatorBrush" Color="#FF606060"/>
    <SolidColorBrush x:Key="FeedbackExportHeaderForegroundBrush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="FeedbackExportMessageForegroundBrush" Color="#50FFFFFF"/>
    <SolidColorBrush x:Key="FeedbackExportSuccessForegroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="FeedbackExportErrorForegroundBrush" Color="#FFDB000C"/>
    
    <!-- Search View -->

    <SolidColorBrush x:Key="SearchViewSeparatorBrush" Color="#FF606060"/>
    <SolidColorBrush x:Key="SearchViewBusyIndeterminateBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="SearchViewNoResultsForegroundBrush" Color="#30FFFFFF"/>

    <SolidColorBrush x:Key="SearchViewItemForegroundBrush" Color="#FFE3E3E3"/>
    <SolidColorBrush x:Key="SearchViewItemMatchForegroundBrush" Color="#FFC2C4CB"/>
    <SolidColorBrush x:Key="SearchViewItemMatchHighlightBrush" Color="#FF42B649"/>

    <SolidColorBrush x:Key="SearchViewItemBackgroundBrush" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="SearchViewItemBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="SearchViewItemMouseOverBackgroundBrush" Color="#FF42B649"/>
    <SolidColorBrush x:Key="SearchViewItemMouseOverBorderBrush" Color="Transparent"/>

    <!-- Source Code View -->

    <SolidColorBrush x:Key="SourceCodeViewBackgroundBrush" Color="#FF181818"/>

    <!-- Tips View -->

    <Color x:Key="SciChartTipsColor">#FFE3E3E3</Color>
    <SolidColorBrush x:Key="SciChartTipsBrush" Color="#FFE3E3E3"/>

    <SolidColorBrush x:Key="TipsBorderBackground" Color="#FF2C3135"/>
    <SolidColorBrush x:Key="TipsButtonBackground" Color="#FF2C3135"/>

    <SolidColorBrush x:Key="InfoPanelBackgroundBrush" Color="#1C1C1F"/>

</ResourceDictionary>