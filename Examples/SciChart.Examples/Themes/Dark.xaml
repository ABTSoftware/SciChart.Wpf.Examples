    <SolidColorBrush x:Key="ExtraThemeStrokeBrush_1" Color="#FFFF1919"/>
    <SolidColorBrush x:Key="ExtraThemeTransparentStrokeBrush_1" Color="#77FF1919"/>
    <SolidColorBrush x:Key="ExtraThemeStrokeBrush_2" Color="#FF1964FF"/>
    <SolidColorBrush x:Key="ExtraThemeTransparentStrokeBrush_2" Color="#771964FF"/>
    <SolidColorBrush x:Key="ExtraThemeStrokeBrush_3" Color="#FFFF9B27"/>
    <SolidColorBrush x:Key="ExtraThemeTransparentStrokeBrush_3" Color="#77FF9B27"/>

    <LinearGradientBrush x:Key="ExtraThemeFillGradient_1" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FFFF1919" />
        <GradientStop Offset="1" Color="#77FF1919" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ExtraThemeFillGradient_2" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FF1964FF" />
        <GradientStop Offset="1" Color="#771964FF" />
    </LinearGradientBrush>

    <Color x:Key="ExtraThemeStrokeColor_4">#FF00BFFF</Color>
    <Color x:Key="ExtraThemeTransparentStrokeColor_4">#7700BFFF</Color>
    <Color x:Key="ExtraThemeStrokeColor_5">#FF0000FF</Color>
    <Color x:Key="ExtraThemeTransparentStrokeColor_5">#770000FF</Color>
    <Color x:Key="ExtraThemeStrokeColor_6">#FFE1E113</Color>
    <Color x:Key="ExtraThemeTransparentStrokeColor_6">#77E1E113</Color>

    <SolidColorBrush x:Key="ExtraThemeStrokeBrush_4" Color="#FF00BFFF"/>
    <SolidColorBrush x:Key="ExtraThemeTransparentStrokeBrush_4" Color="#7700BFFF"/>
    <SolidColorBrush x:Key="ExtraThemeStrokeBrush_5" Color="#FF0000FF"/>
    <SolidColorBrush x:Key="ExtraThemeTransparentStrokeBrush_5" Color="#770000FF"/>
    <SolidColorBrush x:Key="ExtraThemeStrokeBrush_6" Color="#FFE1E113"/>
    <SolidColorBrush x:Key="ExtraThemeTransparentStrokeBrush_6" Color="#77E1E113"/>

    <LinearGradientBrush x:Key="MountainAreaBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#AAFF8D42" />
        <GradientStop Offset="1" Color="#88090E11" />
    </LinearGradientBrush>

    <Color x:Key="PointMarkerStrokeColor">#FFB0C4DE</Color>
    <Color x:Key="PointMarkerFillColor">#FF4682B4</Color>

    <LinearGradientBrush x:Key="NewsBulletGradientForMarkersExample" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.5" Color="#FFF" />
        <GradientStop Offset="1" Color="#77C0D4EE" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ThemedChartBackground" Color="#FF1C1C1F"/>
    <SolidColorBrush x:Key="ThemedTransparentChartBackground" Color="#DF1C1C1F"/>
    <SolidColorBrush x:Key="ThemedSurfaceChartBackground" Color="#FF1C1C1E"/>

    <Color x:Key="ColorMapColor_1">DarkBlue</Color>
