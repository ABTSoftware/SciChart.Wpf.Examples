    <LinearGradientBrush x:Key="Style3DChartBackground" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0.022" Color="#FF3D4703" />
        <GradientStop Offset="0.36" Color="#FF232518" />
        <GradientStop Offset="0.6" Color="#FF13140D" />
        <GradientStop Offset="0.6" Color="#FF13140D" />
        <GradientStop Offset="1" Color="Black" />
    </LinearGradientBrush>

    <!-- SciChart Controls Styles -->

    <Style TargetType="{x:Type s:SciChartSurface}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciStockChart}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciChartDonutSurface}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciChartPieSurface}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciChartRadarSurface}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciChartTernarySurface}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciChartParallelCoordinateSurface}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s3D:SciChart3DSurface}">
        <Setter Property="IsFpsCounterVisible" Value="False"/>
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:SciChartOverview}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

    <Style TargetType="{x:Type s:HeatmapColorMap}">
        <Setter Property="s:ThemeManager.Theme" Value="SciChartv4Dark"/>
    </Style>

</ResourceDictionary>