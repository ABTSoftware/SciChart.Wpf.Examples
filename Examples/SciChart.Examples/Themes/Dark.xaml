                    xmlns:s="http://schemas.abtsoftware.co.uk/scichart"
                    xmlns:s3D="http://schemas.abtsoftware.co.uk/scichart3D">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/SciChart.Examples.ExternalDependencies;component/Themes/Dark.xaml"/>
        <ResourceDictionary Source="/SciChart.Examples.ExternalDependencies;component/Resources/Styles/Controls.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <!-- Examples Series Colors -->

    <Color x:Key="MainThemeStrokeColor">#FF99EE99</Color>
    <Color x:Key="SecondaryThemeStrokeColor">#FF3C6A91</Color>
    <Color x:Key="MainThemeTransparentStrokeColor">#7799EE99</Color>
    <Color x:Key="SecondaryTransparentStrokeColor">#773C6A91</Color>

    <Color x:Key="ExtraThemeStrokeColor_1">#FFFF1919</Color>
    <Color x:Key="ExtraThemeTransparentStrokeColor_1">#77FF1919</Color>
    <Color x:Key="ExtraThemeStrokeColor_2">#FF1964FF</Color>
    <Color x:Key="ExtraThemeTransparentStrokeColor_2">#771964FF</Color>
    <Color x:Key="ExtraThemeStrokeColor_3">#FFFF9B27</Color>
    <Color x:Key="ExtraThemeTransparentStrokeColor_3">#77FF9B27</Color>

    <SolidColorBrush x:Key="MainThemeSolidBrush" Color="#FF99EE99"/>
    <SolidColorBrush x:Key="SecondaryThemeSolidBrush" Color="#FF3C6A91"/>
    <SolidColorBrush x:Key="MainThemeTransparentSolidBrush" Color="#7799EE99"/>
    <SolidColorBrush x:Key="SecondaryThemeTransparentSolidBrush" Color="#773C6A91"/>

    <LinearGradientBrush x:Key="MainFillGradient" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FF99EE99" />
        <GradientStop Offset="1" Color="#7799EE99" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="SecondaryFillGradient" StartPoint="0,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FF3C6A91" />
        <GradientStop Offset="1" Color="#773C6A91" />
